{% extends 'TwigBundle:Exception:layout.html.twig' %}

{% block title %}
    Konsserto - Welcome!
{% endblock title %}
{% block exception_title %}
    Welcome!
{% endblock exception_title %}

{% block additionnalcss %}

    .encart {
        min-height:250px;
    }


    a:active {
		text-decoration: none;
		outline: none;
    }

    a:focus {
		text-decoration: none;
		outline: none;
    }

    a img {
		text-decoration: none;
    }

    .mtop {
		margin-top: 20px !important;
    }

    .mbot {
		margin-bottom: 20px !important;
    }

    .mlef {
		margin-left: 20px !important;
    }

    .mrig {
		margin-right: 20px !important;
    }

    .brig {
		border-right: 1px solid #444;
    }

    .clear {
		clear: both;
    }

    .template {
		width: 1155px;
		margin: 0 auto;
		padding: 10px;
    }

    .separator {
		width: 99%;
		height: 1px;
		background: #ccc;
		margin: 15px 0;
    }

    small {
		border: 1px solid #d2d2d2;
		background: #fff;
		font-size: 11px;
		padding: 18px 5px 5px 5px;
		display: block;
		width: 100%;
		color: #3c3c3c;
    }

    small.success {
		color: #008000;
		border: 1px solid #fff;
		background: rgba(0,255,0,0.15);
    }

    small.error {
		color: #f00;
		border: 1px solid #fff;
		background: rgba(255,0,0,0.15);
    }

    small.flash {
		color: #00a6f0;
		border: 1px solid #fff;
		background: rgba(0,160,240,0.15);
		text-align:center;
		font-size:14px;
		padding-bottom:20px;
		margin-bottom:30px;
    }

    small.resize-small {
		width: 99%;
    }

    small.copyright {
		color: #222;
		background: #fff;
    }

    small.copyright a:hover {
		color: #f00;
		text-decoration: underline;
    }

    small.copyright img {
		margin: -2px 5px 0 5px;
    }

    footer {
		width: 100%;
		height: auto;
		background: url('data:image/png;base64,') repeat;
		box-shadow: 0 -2px 5px #aaa;
		border-top: 2px solid #fff;
		font-weight: normal;
		padding-bottom: 0px;
		margin-top: 50px;
		padding-bottom: 30px;
    }

    footer h3 {
		font-size: 16px;
		margin-bottom: 20px;
    }

    footer h3 img {
		margin-top: -5px;
    }

    footer .col {
        display:inline-block;
        width: 200px;
        margin-right: 25px;
        margin-top: 20px;
        color: #fff;
        text-align:left;
    }

    footer .col img {
        float: left;
        margin-right: 5px;
    }

    footer .col p {
        margin: 0;
    }

    footer .col li {
        margin-bottom: 10px;
    }

    footer .col li a {
        color: #fff;
        font-weight: normal;
    }

    footer .col li a:hover {
        color: #FF8A00;
    }

    footer .col h3 {
        color: #fff;
    }

    footer .coli {
         margin-left: 5px;
    }

    footer .colp {
        width: 350px;
    }

    #ks-start li {
        margin:0;
    }

    #ks-start a {
        display:inline-block;
        width:338px;
        margin-left:20px;
        height:200px;
        border:2px solid #DDD;
        box-shadow:0 0 2px #777;
        -moz-box-shadow:0 0 2px #777;
        -webkit-box-shadow:0 0 2px #777;
        text-align:center;
        font-weight:normal;

        background: #ffffff; /* Old browsers */
        background: -moz-linear-gradient(top, #ffffff 0%, #f6f6f6 47%, #ededed 100%); /* FF3.6+ */
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(47%,#f6f6f6), color-stop(100%,#ededed)); /* Chrome,Safari4+ */
        background: -webkit-linear-gradient(top, #ffffff 0%,#f6f6f6 47%,#ededed 100%); /* Chrome10+,Safari5.1+ */
        background: -o-linear-gradient(top, #ffffff 0%,#f6f6f6 47%,#ededed 100%); /* Opera 11.10+ */
        background: -ms-linear-gradient(top, #ffffff 0%,#f6f6f6 47%,#ededed 100%); /* IE10+ */
        background: linear-gradient(to bottom, #ffffff 0%,#f6f6f6 47%,#ededed 100%); /* W3C */
        filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#ededed',GradientType=0 ); /* IE6-9 */
    }

    #ks-start a h2 {
        margin-top:70px;
        font-size:30px;
        color:#AAAAAA;
    }

    #ks-start a p {
        font-size:13px;
        color:#AAAAAA;
    }


    #ks-start a:hover {
        background: #FF8A00;
        box-shadow:0 0 2px #FF8A00;
        -moz-box-shadow:0 0 2px #FF8A00;
        -webkit-box-shadow:0 0 2px #FF8A00;
        border:2px solid white;
        text-decoration:none;
    }

    #ks-start a:hover h2 {
        color:white;
    }

    #ks-start a:hover p {
        color:white;
    }

{% endblock %}


{% block picto %}
    <img src="data:image/png;base64,"/>
{% endblock %}



