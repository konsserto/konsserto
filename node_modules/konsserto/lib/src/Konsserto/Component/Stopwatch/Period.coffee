#
# Period
#
# @author Marvin Frachet <marvin@konsserto.com>
#
class Period


	constructor: (@startTime)->
		@endTime = new Date().getTime()

	getDuration: ()->
		return @endTime - @startTime

	getStartTime: ()->
		return @startTime

	getEndTime: ()->
		return @endTime


module.exports = Period